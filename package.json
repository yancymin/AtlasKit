  },
  "scripts": {
    "build/docker/image": "build/bin/build.docker.image.sh",
    "bootstrap": "lerna exec -- git clean -Xdf . && lerna bootstrap",
    "bootstrap/single": "./build/bin/bootstrap.single.sh",
    "bootstrap/single/with-deps": "./build/bin/bootstrap.single.with.deps.sh",
    "browserstack/tunnel/start": "./build/bin/browserstack.tunnel.start.sh",
    "browserstack/tunnel/stop": "./build/bin/browserstack.tunnel.stop.sh",
    "clean": "lerna exec -- git clean -Xdf . && rm -rf node_modules",
    "commit": "./build/bin/commit.sh",
    "create": "./build/bin/create.new.component.sh",
    "create-ts": "./build/bin/create.new.ts.component.sh",
    "help": "readme-help",
    "health-check": "./build/bin/health-check-scripts/health.check.js",
    "identity-obj-proxy": "^3.0.0",
    "lint": "yarn run validate/lint",
    "postinstall": "./build/bin/postinstall.sh",
    "docs": "lerna exec -- ../../build/bin/generate.readme.sh",
    "pr/storybook": "./build/bin/storybook.static.pr.sh",
    "prepublish/all": "lerna run prepublish",
    "prepublish/single": "./build/bin/prepublish.single.sh",
    "release": "./build/bin/release.process.bash",
    "release/announce": "./build/bin/release.announce.js",
    "release/check/is-workspace-clean": "git diff --exit-code || (chalk --no-stdin -t \"{red \n^ Workspace is not clean!}\"; exit 1)",
    "release/check/can-ff": "git pull --ff-only --no-edit origin || (chalk --no-stdin -t \"{blue We are not the leading build - nothing to do here!}\"; exit 1)",
    "release/registry": "./build/bin/release.registry.sh || (chalk --no-stdin -t \"{red Registry generation failed!}\"; exit 1)",
    "release/storybooks": "./build/bin/release.storybooks.sh || (chalk --no-stdin -t \"{red Storybook generation failed!}\"; exit 1)",
    "storybook": "./build/bin/storybook.single.sh",
    "storybook/single": "chalk -t '{yellow `yarn run storybook/single` is deprecated, use `yarn run storybook` instead}' && ./build/bin/storybook.single.sh",
    "storybook/static": "node --max_old_space_size=4096 ./node_modules/.bin/build-storybook -c ./build/storybook",
    "storybook/static/single": "./build/bin/storybook.static.single.sh",
    "test/base": "mocha --colors",
    "test/wc": "./build/bin/test.sh",
    "test/browserstack": "./build/bin/test.browserstack.sh",
    "test/wc/single": "./build/bin/test.single.sh",
    "test/browserstack/single": "./build/bin/test.single.browserstack.sh",
    "test/wc/single/watch": "NODE_ENV=development ./build/bin/test.single.watch.sh",
    "test/wc/watch": "NODE_ENV=development ./build/bin/test.watch.sh",
    "test": "./build/bin/test.react.sh",
    "test/codemod": "jest codemod",
    "test/unit": "./build/bin/test.unit.sh",
    "test/jest": "lerna run jest",
    "test/single": "./build/bin/test.react.single.sh",
    "test/single/watch": "NODE_ENV=development ./build/bin/test.react.single.watch.sh",
    "test/watch": "NODE_ENV=development ./build/bin/test.react.watch.sh",
    "validate": "npm ls >/dev/null && lerna exec -- npm ls >/dev/null",
    "validate/eslint": "./build/bin/lint.eslint.sh",
    "validate/eslint/changed": "./build/bin/lint.eslint.changed.sh",
    "validate/eslint/changed/fix": "./build/bin/lint.eslint.changed.sh --fix",
    "validate/eslint/fix": "./build/bin/lint.eslint.sh --fix",
    "validate/lint": "run-p -cl validate/tslint validate/eslint validate/flow",
    "validate/lint-changed": "run-p -l validate/eslint/changed validate/tslint/changed",
    "validate/lint-changed/fix": "run-p -l validate/eslint/changed/fix validate/tslint/changed/fix",
    "validate/lint/fix": "run-p -l validate/eslint/fix validate/tslint/fix",
    "validate/tslint": "lerna run lint",
    "validate/tslint/changed": "./build/bin/lint.tslint.changed.sh",
    "validate/tslint/changed/fix": "./build/bin/lint.tslint.changed.sh --fix",
    "validate/tslint/fix": "lerna run -- lint -- --fix",
    "validate/package.json": "pjv -q && lerna exec -- pjv -q",
    "validate/typescript": "lerna run validate/typescript",
    "validate/flow": "flow check"
  }
